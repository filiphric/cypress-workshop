{
  "name": "cypress-workshop",
  "version": "1.0.0",
  "description": "Cypress workshop",
  "main": "index.js",
  "scripts": {
    "cy:open:todomvc": "cypress open --project ./tests/todomvc",
    "cy:run:todomvc": "cypress run --project ./tests/todomvc",
    "cy:open:realworld": "cypress open --project ./tests/realworld",
    "cy:run:realworld": "cypress run --project ./tests/realworld",
    "postinstall": "run-s install:todomvc install:realworld-client install:realworld-server",
    "reset:todomvc": "cd apps/todomvc && npm run reset",
    "install:realworld-client": "cd apps/realworld/client && npm ci",
    "install:realworld-server": "cd apps/realworld/server && npm ci",
    "install:todomvc": "cd apps/todomvc && npm install",
    "start:todomvc": "cd apps/todomvc && npm start",
    "start:realworld": "concurrently npm:start:client npm:start:server",
    "start:client": "cd apps/realworld/client && npm start",
    "start:server": "cd apps/realworld/server && npm start"
  },
  "devDependencies": {
    "cypress": "3.4.1",
    "concurrently": "4.1.2",
    "debug": "4.1.1",
    "http-server": "0.11.1",
    "json-server": "0.15.1",
    "json-server-reset": "1.2.0",
    "start-server-and-test": "1.10.5",
    "npm-run-all": "4.1.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
